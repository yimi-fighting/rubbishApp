view {
  background-color: #f7f7f7;
}
swiper .swiper-item,
swiper image {
  width: 100%;
  height: 100%;
}
.search_contain {
  background-color: white;
  position: relative;
  height: 40px;
  box-sizing: border-box;
  margin: 20px 10px 20px 10px;
  border-radius: 30px;
}
.search_contain .icon-fangdajing {
  background-color: white;
  position: absolute;
  left: 15px;
  line-height: 40px;
  color: #00cb79;
}
.search_contain .search {
  padding-left: 40px;
  position: absolute;
  top: 10px;
  font-size: 14px;
}
.photograph {
  position: relative;
}
.photograph .photo_img {
  width: 100%;
}
.photograph .photo {
  position: relative;
  width: 70px;
  height: 70px;
  background-color: #00cb79;
  border-radius: 50%;
  margin: 0 auto;
}
.photograph .photo .icon-weibiaoti1 {
  position: absolute;
  left: 35%;
  top: 35%;
  width: 20px;
  color: white;
  height: 20px;
  font-size: 40px;
  background-color: #00cb79;
  -webkit-transform: translate(-50%, -50%);
          transform: translate(-50%, -50%);
}
.photograph .text {
  position: absolute;
  left: 50%;
  -webkit-transform: translate(-50%);
          transform: translate(-50%);
  margin-top: 10px;
  color: #00cb79;
  font-size: 14px;
  font-weight: 600;
  height: auto;
}
.new_contain {
  height: auto;
  margin-top: 50px;
}
.new_contain .news {
  font-size: 25px;
  margin: 20px;
  border-left: 3px red solid;
  padding-left: 10px;
}
.new_contain .new_item {
  border: 1px dashed black;
  border-radius: 10px;
  margin: 10px;
  padding: 5px;
  display: flex;
}
.new_contain .new_item .left .title {
  display: -webkit-box;
  -webkit-box-orient: vertical;
  overflow: hidden;
  -webkit-line-clamp: 1;
  text-overflow: ellipsis;
}
.new_contain .new_item .left .time {
  font-size: 14px;
  line-height: 25px;
  color: dimgray;
}
.new_contain .new_item .left .description {
  font-size: 13px;
  color: darkgray;
  display: -webkit-box;
  overflow: hidden;
  -webkit-line-clamp: 3;
  text-overflow: ellipsis;
  -webkit-box-orient: vertical;
}
.new_contain .new_item .right {
  margin-left: 10px;
}
.new_contain .new_item .right .picUrl {
  width: 70px;
  height: 70px;
  padding-top: 15px;
}
.new_contain .new_item .right .picUrl ._img {
  width: 100%;
  height: 100%;
  object-fit: fill;
}
.photo_item {
  height: 50px;
  display: flex;
  line-height: 50px;
  background-color: #55bf53;
  color: white;
  border-bottom: 1px dashed #55bf53;
  padding-left: 10px;
}
.photo_item ._div {
  background-color: #55bf53;
}
.photo_item .fenge {
  margin: 0 5px;
}
.photo_item .icon {
  line-height: 50px;
  margin-right: 5px;
}
.collapse_container .collapse_item .content {
  display: flex;
  height: 50px;
  padding: 0 10px;
  line-height: 50px;
  justify-content: space-between;
}
.collapse_container .collapse_item .content .type {
  height: 30px;
  border-radius: 30px;
  color: white;
  box-sizing: border-box;
  margin-top: 10px;
  line-height: 30px;
  padding: 0 10px;
}
.collapse_container .collapse_item .content .chuyulaji {
  background-color: #5dab46;
}
.collapse_container .collapse_item .content .qitalaji {
  background-color: #747371;
}
.collapse_container .collapse_item .content .kehuishouwu {
  background-color: #0168b7;
}
.collapse_container .collapse_item .content .youhailaji {
  background-color: #f53327;
}

